<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="FoundOps.Core.Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="FoundOps.Server" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
          <Binding name="Endpoint2" endpointName="Endpoint2" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
      <!--<InputEndpoint name="Endpoint2" protocol="https" port="443" certificate="app.foundops.com" />-->
      <InputEndpoint name="Endpoint2" protocol="https" port="443" certificate="test.foundops.com" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <Certificates>
      <!--<Certificate name="app.foundops.com" storeLocation="LocalMachine" storeName="My" />-->
      <Certificate name="test.foundops.com" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WebRole>
</ServiceDefinition>