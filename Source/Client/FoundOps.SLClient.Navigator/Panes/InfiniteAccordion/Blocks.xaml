                    <InfiniteAccordion:ContextBorder.HeaderContent>
                        <TextBlock Text="{Binding Name}" Foreground="White" Margin="5,0" />
                    </InfiniteAccordion:ContextBorder.HeaderContent>
                </InfiniteAccordion:ContextBorder>
                <InfiniteAccordion:ContextBorder ContextType="FoundOps.Core.Models.CoreEntities.RecurringService" BorderBrush="{StaticResource ServiceBrush}" DeleteButtonVisibility="Collapsed">
                    <InfiniteAccordion:ContextBorder.HeaderContent>
                        <TextBlock Margin="5,0" Text="{Binding ServiceTemplate.Name}" Foreground="White" />
                    </InfiniteAccordion:ContextBorder.HeaderContent>
                </InfiniteAccordion:ContextBorder>
                <InfiniteAccordion:ContextBorder ContextType="FoundOps.Core.Models.CoreEntities.UserAccount" BorderBrush="{StaticResource PermissionBrush}" DeleteButtonVisibility="Collapsed">
                    <InfiniteAccordion:ContextBorder.HeaderContent>
                        <TextBlock Text="{Binding DisplayName}" Foreground="White"/>
                    </InfiniteAccordion:ContextBorder.HeaderContent>
                </InfiniteAccordion:ContextBorder>
                <InfiniteAccordion:ContextBorder ContextType="FoundOps.Core.Models.CoreEntities.Vehicle" BorderBrush="{StaticResource VehiclesBrush}" DeleteButtonVisibility="Collapsed">
                    <InfiniteAccordion:ContextBorder.HeaderContent>
                        <TextBlock Margin="5,0" Text="{Binding VehicleId}" Foreground="White" />
                    </InfiniteAccordion:ContextBorder.HeaderContent>
                </InfiniteAccordion:ContextBorder>
            </InfiniteAccordion:InfiniteAccordionView.ContextBorderControls>
        </InfiniteAccordion:InfiniteAccordionView>
    </Grid>
</UserControl>
